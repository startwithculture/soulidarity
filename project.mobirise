              "font-size": "(@fontSize * 1px)",
              "line-height": "((@fontSize * 1px) + 20px)"
            },
            ".animated-element": {
              "color": "@wordsColor",
              "font-size": "(@fontSize * 1px)",
              "line-height": "((@fontSize * 1px) + 20px)",
              "& when (@showPoint)": {
                "&:after": {
                  "content": "\".\"",
                  "color": "@colorPoint"
                }
              }
            },
            ".typed-cursor": {
              "color": "@wordsColor",
              "font-size": "(@fontSize * 1px)"
            },
            "& when (@underline)": {
              ".mbr-section-title, .animated-element": {
                "text-decoration": "underline"
              }
            },
            "@media (max-width: 991px)": {
              ".mbr-section-title, .animated-element, .typed-cursor": {
                "font-size": "100px",
                "line-height": "100px"
              }
            },
            "@media (max-width: 767px)": {
              ".mbr-section-title, .animated-element, .typed-cursor": {
                "font-size": "80px",
                "line-height": "80px"
              }
            },
