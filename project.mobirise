          "alias": "mobirise4-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "!isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".nav-tabs .nav-item.open .nav-link:focus, .nav-tabs .nav-link.active:focus": {
              "outline": "none"
            },
            ".nav-tabs": {
              "-webkit-flex-wrap": "wrap",
              "flex-wrap": "wrap",
              "border-bottom": "0px solid lightgray",
              ".nav-item": {
                "padding": ".0rem"
              }
            },
            ".nav-link": {
              "padding": ".4rem 0.8rem"
            },
            ".nav-tabs .nav-link": {
              "color": "@tabsColor",
              "font-style": "normal",
              "font-weight": "500",
              "transition": "all .5s",
              "background": "transparent",
              "border": "none",
              "border-radius": "0 !important",
              "border-bottom": "2px solid @borderColor",
              "&:hover": {
                "cursor": "pointer",
                "color": "@activeTabColor",
                "border-bottom": "2px solid @activeTabColor"
              },
              "&.active": {
                "font-weight": "500",
                "color": "@activeTabColor",
                "font-style": "normal",
                "border-bottom": "2px solid @activeTabColor"
              }
            },
            "P": {
              "color": "#767676"
            },
            "@media (max-width: 576px)": {
              ".nav-tabs": {
                "flex-direction": "column",
                "justify-content": "center"
              },
              ".mbr-text, .nav-link": {
                "text-align": "center"
              }
            }
